#include<bits/stdc++.h>
  using namespace std;
    int main()
    {
     string s;
     int m,j,i;
     cin>>m>>j;
     cin>>s;
     for(i=0;i<j;i++)

     {
      if(i==0)
      s[i]='1';
      else if(s[i]=='0')
      {
       j=j+1;
       continue;
      }
      else
      s[i]='0';
     }
     cout<<s;






    }
